#include <stdio.h>
#include <string.h>

int main() {
    char str1[50], str2[50], str3[100];
    int len, comparison_result;

  
    printf("Enter a string for length calculation: ");
    fgets(str1, sizeof(str1), stdin);
    str1[strcspn(str1, "\n")] = 0; 
    len = strlen(str1);
    printf("Length of \"%s\" is: %d\n\n", str1, len);

    
    printf("Enter a string to copy: ");
    fgets(str2, sizeof(str2), stdin);
    str2[strcspn(str2, "\n")] = 0; 
    strcpy(str3, str2); 
    printf("Original string: \"%s\"\n", str2);
    printf("Copied string: \"%s\"\n\n", str3);

   
    printf("Enter the first string for comparison: ");
    fgets(str1, sizeof(str1), stdin);
    str1[strcspn(str1, "\n")] = 0; 
    printf("Enter the second string for comparison: ");
    fgets(str2, sizeof(str2), stdin);
    str2[strcspn(str2, "\n")] = 0; 
    comparison_result = strcmp(str1, str2);
    if (comparison_result == 0) {
        printf("Strings are equal.\n\n");
    } else if (comparison_result < 0) {
        printf("\"%s\" is less than \"%s\".\n\n", str1, str2);
    } else {
        printf("\"%s\" is greater than \"%s\".\n\n", str1, str2);
    }

 
    printf("Enter the first string for concatenation: ");
    fgets(str1, sizeof(str1), stdin);
    str1[strcspn(str1, "\n")] = 0; 
    printf("Enter the second string to concatenate: ");
    fgets(str2, sizeof(str2), stdin);
    str2[strcspn(str2, "\n")] = 0; 
    strcat(str1, str2);
    printf("Concatenated string: \"%s\"\n", str1);

    return 0;
}
