#include<stdio.h>
int linear_search(int arr[],int n,int key) {
           
            for( int i=0;i<n;i++) {
                if(arr[i] == key) {
                    return i;
                }
             
                 
              }
               return -1;
            }
          
int main() {
  int arr[] = {10,20,30,40,60,70,80,90,100};
 int n = sizeof(arr)/sizeof(arr[0]);
 int key = 50;
 int result =  linear_search(arr, n, key);
 if(result == -1) {
    printf("the element %d is not present in array.\n", key);
 }
 else {
     printf(" %d element is present in the %d index.\n", key, result);

 }



    return 0;
}
